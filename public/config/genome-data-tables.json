        "Expression": {
            "displayName": "Expression Data",
            "description": "Gene expression measurements",
            "icon": "bi-graph-up",
            "settings": {
                "enableSelection": true,
                "density": "compact"
            },
            "categories": [
                {
                    "id": "gene",
                    "name": "Gene Info",
                    "icon": "bi-database",
                    "color": "#6366f1",
                    "defaultVisible": true
                },
                {
                    "id": "expression",
                    "name": "Expression Values",
                    "icon": "bi-graph-up",
                    "color": "#ef4444",
